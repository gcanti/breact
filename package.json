{
  "name": "tcomb-react-bootstrap",
  "version": "0.0.3",
  "description": "Fast prototyping: Bootstrap + React.js + debugging support",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build-playground": "jsx -w -x jsx --no-cache-dir playground playground",
    "build": "watchify index.js -o build/tcomb-react-bootstrap.js -v -r ./index:tcomb-react-bootstrap -r react -r tcomb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/tcomb-react-bootstrap.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/tcomb-react-bootstrap/issues"
  },
  "homepage": "https://github.com/gcanti/tcomb-react-bootstrap",
  "dependencies": {
    "react-bootstrap": "*",
    "react": "*",
    "tcomb": "*"
  },
  "devDependencies": {
    "reactify": "~0.4.0",
    "watchify": "~0.4.1"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "tags": [
    "react",
    "bootstrap",
    "tcomb"
  ]
}
